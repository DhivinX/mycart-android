<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item>
        <shape
            android:shape="oval">

            <solid
                android:color="#F2F6F7"/>

            <size
                android:width="120dp"
                android:height="120dp"/>
        </shape>
    </item>

    <item
        android:top="10dp"
        android:bottom="10dp"
        android:left="10dp"
        android:right="10dp">
        <bitmap android:src="@drawable/icon_camera" />
    </item>
</layer-list>